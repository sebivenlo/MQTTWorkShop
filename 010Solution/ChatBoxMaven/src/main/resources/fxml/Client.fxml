<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="561.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chatbox.FXMLDocumentController">
    <children>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <Button fx:id="btnSend" layoutX="504.0" layoutY="399.0" mnemonicParsing="false" onAction="#Publish" prefHeight="100.0" prefWidth="79.0" text="Send" />
      <TextArea fx:id="tbMessage" layoutX="34.0" layoutY="367.0" prefHeight="157.0" prefWidth="459.0" />
      <TextField id="userIdField" fx:id="tbUserId" layoutX="102.0" layoutY="27.0" />
      <TextField fx:id="tbIpAdress" layoutX="102.0" layoutY="62.0" />
      <Label layoutX="32.0" layoutY="31.0" text="UserId:" />
      <Label layoutX="34.0" layoutY="66.0" text="ip-adress:" />
      <HBox layoutX="34.0" layoutY="129.0" prefHeight="36.0" prefWidth="636.0">
         <children>
            <HBox>
               <children>
                  <Label alignment="BASELINE_CENTER" prefHeight="16.0" prefWidth="93.0" text="Messages :" />
                  <TextField fx:id="tbTopic" alignment="BASELINE_CENTER" />
                  <Button fx:id="btnConnect" alignment="BASELINE_CENTER" mnemonicParsing="false" onAction="#Connect" text="Connect" />
                  <Button fx:id="btnDisconnect" alignment="BASELINE_CENTER" mnemonicParsing="false" onAction="#Disconnect" text="Disconnect" />
                  
                  <Button fx:id="btnJoin" alignment="BASELINE_CENTER" mnemonicParsing="false" onAction="#Subscribe" text="Subscribe" />
               </children>
            </HBox>
         </children>
      </HBox>
    </children>
</AnchorPane>
